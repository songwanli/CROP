<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobilebank.develop" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PTBrowser">
        <param name="android-package" value="asp.citic.ptframework.plugin.browser.PTBrowser" />
    </feature>
    <feature name="PTRequest">
        <param name="android-package" value="asp.citic.ptframework.plugin.request.PTRequest" />
    </feature>
    <feature name="PTKeyboards">
        <param name="android-package" value="asp.citic.ptframework.plugin.keyboards.PTKeyboards" />
    </feature>
    <feature name="example">
        <param name="android-package" value="package.example" />
    </feature>
    <allow-intent href="market:*" />
    <name>CROP</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
